{
  "name": "@PheasantDevil/test-storybook",
  "version": "1.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/PheasantDevil/test-storybook.git"
  },
  "homepage": "https://pheasantdevil.github.io/test-storybook/",
  "license": "UNLICENSED",
  "scripts": {
    "start": "start-storybook -p 6006 -s public",
    "build": "build-storybook",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write . ",
    "format:check": "prettier --check .",
    "deploy:local": "gh-pages -d storybook-static",
    "deploy": "yarn build && yarn deploy:local"
  },
  "dependencies": {
    "@semantic-release/commit-analyzer": "9.0.2",
    "@types/node": "16.11.43",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "4.7.4"
  },
  "devDependencies": {
    "@babel/core": "7.18.6",
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@semantic-release/release-notes-generator": "semantic-release/release-notes-generator",
    "@storybook/addon-actions": "6.5.9",
    "@storybook/addon-essentials": "6.5.9",
    "@storybook/addon-interactions": "6.5.9",
    "@storybook/addon-links": "6.5.9",
    "@storybook/builder-webpack5": "6.5.9",
    "@storybook/manager-webpack5": "6.5.9",
    "@storybook/node-logger": "6.5.9",
    "@storybook/preset-create-react-app": "4.1.2",
    "@storybook/react": "6.5.9",
    "@storybook/testing-library": "0.0.13",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.5.2",
    "babel-loader": "8.2.5",
    "babel-plugin-named-exports-order": "0.0.2",
    "gh-pages": "4.0.0",
    "husky": "8.0.1",
    "prettier": "2.7.1",
    "prop-types": "15.8.1",
    "sass": "1.53.0",
    "semantic-release": "^19.0.3",
    "web-vitals": "2.1.4",
    "webpack": "5.73.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
